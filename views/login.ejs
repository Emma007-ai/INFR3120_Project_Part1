<%- include('partials/header') %>

<h1>Login</h1>

<form method="POST" action="/login">
  <div>
    <label>Username</label>
    <input name="username" required>
  </div>
  <div>
    <label>Password</label>
    <input name="password" type="password" required>
  </div>
  <button type="submit">Login</button>
</form>

